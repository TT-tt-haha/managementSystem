简超强的vue框架\",\r\n      login_form: {\r\n        user_name: \"\",\r\n        user_password: \"\",\r\n      },\r\n      remember_password: false,\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      if(this.login_form.user_name=='admin'&&this.login_form.user_password=='1234'){\r\n        sessionStorage.setItem(\"user_login\", \"login\");\r\n        console.log(this.login_form.user_name);\r\n        sessionStorage.setItem(\"user_label\", \"super_admin\");\r\n        this.$message.success(\"登录成功\");\r\n        setTimeout(() => {\r\n          this.$router.push(\"/home\");\r\n        }, 100);\r\n      }else{\r\n        setTimeout(() => {\r\n          this.$router.push(\"/login\");\r\n        }, 100);\r\n      }\r\n\r\n    },\r\n    to(link){\r\n      console.log(link)\r\n      this.$router.push(link)\r\n    },\r\n    setting_web() {\r\n      this.color_form = {\r\n        aside_background_color: \"#000000\",\r\n        aside_text_color: \"#ffffff\",\r\n        aside_icon_color: \"#ffffff\",\r\n        aside_active_text_color: \"#67C23A\",\r\n        aside_title: \"EuiAdmin\",\r\n        aside_small_title: \"Eui\",\r\n        aside_width: 200,\r\n        aside_small_width: 65,\r\n        head_icon_color: \"#606266\",\r\n        head_background_color: \"#fff\",\r\n        head_text_color: \"#606266\",\r\n        head_active_text_color: \"#303133\",\r\n        head_height: 60,\r\n        main_background_color: \"#f2f6fc\",\r\n      };\r\n      this.$cookies.set(\"setting\", JSON.stringify(this.color_form));\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.$cookies.get(\"setting\") == null) {\r\n      this.setting_web();\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\nh1 {\r\n  color: #606266;\r\n}\r\np {\r\n  color: #606266;\r\n}\r\n#login {\r\n  min-height: 100vh;\r\n}\r\n#form_space {\r\n  border-radius: 10px;\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  margin: auto;\r\n  height: 400px;\r\n  width: 400px;\r\n}\r\n</style>"],"sourceRoot":"src/views/login"}]}